# .env.example - Copy this to .env and customize your values
# For Docker deployment, these are used by docker-compose.yml

# PostgreSQL Database Configuration
POSTGRES_DB=scraper
POSTGRES_USER=scraper_user
POSTGRES_PASSWORD=your_secure_password_here

# Flask Configuration  
SECRET_KEY=change-this-to-a-random-secret-key

# Email Configuration (Resend API) - Optional
RESEND_API_KEY=your_resend_api_key_here

# NOTE: DATABASE_URL is automatically constructed in docker-compose.yml
# You don't need to set it manually for Docker deployment
